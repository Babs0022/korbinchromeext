{
  "manifest_version": 3,
  "name": "VibePilot",
  "version": "1.0",
  "description": "Your AI agent for vibe-coding platforms.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background/index.js"
  }
}
